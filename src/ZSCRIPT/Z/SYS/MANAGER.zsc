class ZManager:Thinker{
    static
    ZManager Instance(){
        ThinkerIterator it=ThinkerIterator.Create("ZManager", STAT_STATIC);
        ZManager p=ZManager(it.Next());
        if(!p)p=new("ZManager").Init();
        return p;
    }

    private
    ZManager Init(){
        ChangeStatNum(STAT_STATIC);
        //World State
        WorldState=new("ZWorldState");
        InitRegistries();
        InitEvents();
        return self;
    }
}