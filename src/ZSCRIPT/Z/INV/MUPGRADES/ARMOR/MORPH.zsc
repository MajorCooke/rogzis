extend
class ZMAU_Morph{
    override
    double ModifyDamage(Actor Inflictor, Actor Source, double Damage, ZDamageType DamageType, int Flags, double Angle){
        if(LastDmgType==DamageType)
            Damage*=1-MorphAbsorption;
        else LastDmgType=DamageType;
        return Damage;
    }
}