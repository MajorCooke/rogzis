extend
class ZArmor{
	protected
	bool //Fall Absorption
		 MidAir;

	virtual
	void SoftenFall(ZPlayer Player){
		console.printf(Player.Pos.Z-Player.GetZAt()+Player.Vel.Z.."");
		if (Player.Pos.Z-Player.GetZAt()+Player.Vel.Z<=30 && Player.Vel.Z<=-ThrustPower)
			if (MidAir){
				MidAir=false;
				Player.Vel.Z+=ThrustPower;
			}
		else MidAir=true;
	}
}