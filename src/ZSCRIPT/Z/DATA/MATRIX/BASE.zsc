class ZMatrixBase abstract{
    Array<double> Values;

    virtual
    ZMatrixBase Clone() const{
        ZMatrixBase Result=ZMatrixBase(new(GetClassName()));
        Result.Values.Copy(Values);
        return Result;
    }
}

